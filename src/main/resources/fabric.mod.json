{
	"schemaVersion": 1,
	"id": "bob-lock",
	"version": "${version}",
	"name": "Bob Lock",
	"description": "Happy little bobs! Make the vanilla view bobbing just a little more happy! :D",
	"authors": [
		"loonsthemoons"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/loonsthemoons/Bob-Lock/"
	},
	"license": "CC0-1.0",
	"icon": "assets/bob-lock/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.loons.BobLock"
		],
		"client": [
			"dev.loons.BobLockClient"
		],
		"modmenu": [
			"dev.loons.ModMenuIntegration"
		]
	},
	"mixins": [
		"bob-lock.mixins.json",
		{
			"config": "bob-lock.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "${minecraft_dependency}",
		"java": ">=${java_version}",
		"fabric-api": "*"
